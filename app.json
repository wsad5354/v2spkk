{
  "name": "V2SPKK-Heroku部署",
  "description": "Deploy SPKK to Heroku.",
  "keywords": [
    "V2ray"
  ],
  "env": {
    "AppName": {
      "description": "请输入最上方填写的App Name，用于订阅链接,必填!",
      "value": ""
    },
    "Subscribe_Address": {
      "description": "请输入查看订阅的地址,前面不用加`/`",
      "value": "subscribe"
    },
      "UUID": {
            "description": "V2Ray UUID",
            "value": "ad806487-2d26-4636-98b6-ab85cc8521f7"
    },
    "AlterID": {
      "description": "AlterID大小，推荐16，数值越大内存占用越高",
      "value": "16",
      "required": false
    },
    "V2_Path": {
      "description": "Path路径，默认留空则随机生成一份",
      "value": "/ws",
      "required": false
    },
    "Anti_Proxy_Path": {
      "description": "反代理的网站地址,默认为百度",
      "value": "https://www.baidu.com",
      "required": true
    }
  },
  "stack": "container"
}
